#include <stdio.h>
int main() {
    int a[10],i,j,n,count;
    scanf("%d",&n);
    for(i=0;i<n;i++)
        scanf("%d",&a[i]);
    for(i=0;i<n;i++){
        count=1;
        for(j=i+1;j<n;j++){
            if(a[i]==a[j]){
                count++;
                a[j]=-1;
            }
        }
        if(a[i]!=-1)
            printf("%d -> %d\n",a[i],count);
    }
    return 0;
}
