#include <stdio.h>
int fact(int n){
    int f=1,i;
    for(i=1;i<=n;i++)
        f*=i;
    return f;
}
int main() {
    int n,temp,sum=0,r;
    scanf("%d",&n);
    temp=n;
    while(n>0){
        r=n%10;
        sum += fact(r);
        n/=10;
    }
    if(sum==temp)
        printf("Strong");
    else
        printf("Not Strong");
    return 0;
}
